const Rosalind_7080 = '';
const Rosalind_2056 = '';
const Rosalind_9885 = '';
const Rosalind_8033 = '';
const Rosalind_4632 = '';

// to count occurances of G in a string

function countG(str) {
    let counter = 0;
    for (let i = 0; i < str.length; i++) {
        if (str[i] == 'G') {
            counter++;
        }        
    }
return counter;
}

// to count occurances of C in a string

function countC(str) {
    let counter = 0;
    for (let i = 0; i < str.length; i++) {
        if (str[i] == 'C') {
            counter++;
        }        
    }
return counter;
}

// to count GS-content in % with 6 decimal places
// toFixed returns string, how to convert to a number? Number(perc).toFixed(6) does not work

function gs(str) {
   let perc = (((countG(str) + countC(str)) / str.length) * 100);
   return perc.toFixed(6);
};

console.log(gs(Rosalind_7080)); // 48.268398
console.log(gs(Rosalind_2056)); // 48.268398
console.log(gs(Rosalind_9885)); // 47.887324
console.log(gs(Rosalind_8033)); // 53.460621
console.log(gs(Rosalind_4632)); // 49.219688

/* 
the answer is typed manually:

Rosalind_8033
53.460621 
*/